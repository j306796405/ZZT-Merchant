        }
        .custom{
            margin-right: 10px;
        }
        .autocomplete_box > div{
            display: inline-block;
        }
        .pic-list .canvas{
            display: table-cell; height: 100px;
            vertical-align: middle;
        }
        .encoded-edit{
            display: inline;
        }
